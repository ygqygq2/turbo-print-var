{
  "name": "turbo-print-var",
  "displayName": "Turbo Print Log - Multi-Language Debug Helper",
  "description": "Intelligent multi-language debug log insertion tool supporting 20+ programming languages",
  "version": "4.0.0",
  "publisher": "ygqygq2",
  "engines": {
    "vscode": "^1.88.0"
  },
  "icon": "resources/images/logo.png",
  "categories": [
    "Debuggers",
    "Testing",
    "Other"
  ],
  "keywords": [
    "debuggers",
    "testing"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Turbo Print Log Configuration",
      "properties": {
        "turbo-print-var.wrapLogMessage": {
          "type": "boolean",
          "default": false,
          "description": "Determine if the log message should be wrapped or not."
        },
        "turbo-print-var.prefix": {
          "type": "string",
          "default": "ðŸš€",
          "description": "Log message prefix symbol."
        },
        "turbo-print-var.suffix": {
          "type": "string",
          "default": ":",
          "description": "Variable name suffix."
        },
        "turbo-print-var.includeFileInfo": {
          "type": "boolean",
          "default": true,
          "description": "Include file name and line number."
        },
        "turbo-print-var.addSemicolon": {
          "type": "boolean",
          "description": "Add semicolon at the end. If not set, uses language default."
        },
        "turbo-print-var.emptyLineBefore": {
          "type": "boolean",
          "default": false,
          "description": "Insert empty line before log."
        },
        "turbo-print-var.emptyLineAfter": {
          "type": "boolean",
          "default": false,
          "description": "Insert empty line after log."
        },
        "turbo-print-var.separator": {
          "type": "string",
          "default": "~",
          "description": "Separator between log elements."
        },
        "turbo-print-var.quote": {
          "type": "string",
          "enum": [
            "\"",
            "'",
            "`"
          ],
          "default": "\"",
          "description": "Quote style: double, single or backtick."
        },
        "turbo-print-var.logFunction": {
          "type": "object",
          "default": {
            "php": "echo"
          },
          "description": "Custom log function per language."
        }
      }
    },
    "commands": [
      {
        "command": "turbo-print-var.insertLog",
        "title": "Turbo Print Log: Insert Log"
      },
      {
        "command": "turbo-print-var.updateLineNumbers",
        "title": "Turbo Print Log: Update Line Numbers"
      },
      {
        "command": "turbo-print-var.commentLogs",
        "title": "Turbo Print Log: Comment Logs"
      },
      {
        "command": "turbo-print-var.uncommentLogs",
        "title": "Turbo Print Log: Uncomment Logs"
      },
      {
        "command": "turbo-print-var.deleteLogs",
        "title": "Turbo Print Log: Delete Logs"
      }
    ],
    "keybindings": [
      {
        "command": "turbo-print-var.insertLog",
        "key": "ctrl+alt+l",
        "mac": "ctrl+alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "turbo-print-var.updateLineNumbers",
        "key": "ctrl+alt+u",
        "mac": "ctrl+alt+u",
        "when": "editorTextFocus"
      },
      {
        "command": "turbo-print-var.commentLogs",
        "key": "alt+shift+c",
        "mac": "alt+shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "turbo-print-var.uncommentLogs",
        "key": "alt+shift+u",
        "mac": "alt+shift+u",
        "when": "editorTextFocus"
      },
      {
        "command": "turbo-print-var.deleteLogs",
        "key": "alt+shift+d",
        "mac": "alt+shift+d",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run clean && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "npm run esbuild --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src ",
    "lint-fix": "eslint --fix src",
    "compile": "npm run esbuild-base -- --sourcemap",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "clean": "rimraf out/",
    "test-compile": "npm run clean && tsc -p tsconfig.mocha.json && npm run compile",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.mocha.json vscode-test",
    "test-grep": "cross-env TS_NODE_PROJECT=tsconfig.mocha.json vscode-test -f",
    "test:suite:mocha": "cross-env TS_NODE_PROJECT=tsconfig.mocha.json npm run test-compile && node out/test/runTests.js",
    "test:suite": "vitest suite --watch=false",
    "test:unit": "vitest unit --watch=false",
    "test:coverage": "vitest run --coverage",
    "prepush": "pnpm run lint",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/chai": "^4.3.14",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/sinon": "^17.0.3",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^1.5.2",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^3.7.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "expect-type": "^0.19.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "jest-mock-vscode": "^3.0.2",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "sinon": "^17.0.1",
    "tmp": "^0.2.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
